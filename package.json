{
  "name": "mcp-evals-harness",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "generate:suites": "npx tsx scripts/generate-suite-index.ts",
    "eval": "npm run generate:suites && npx braintrust eval src/evals/e2e.eval.ts",
    "eval:all": "npm run generate:suites && npx braintrust eval src/evals/run-all.ts",
    "eval:stripe": "EVAL_SUITE=stripe npm run eval",
    "eval:increase": "EVAL_SUITE=increase npm run eval",
    "eval:lithic": "EVAL_SUITE=lithic npm run eval",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.34",
    "@anthropic-ai/sdk": "^0.74.0",
    "@modelcontextprotocol/sdk": "^1.10.0",
    "autoevals": "^0.0.105",
    "braintrust": "^0.0.182",
    "dotenv": "^16.4.5",
    "openai": "^4.47.1",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
